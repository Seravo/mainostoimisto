{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"title": "Fraunces & DM Sans",
	"slug": "typography-preset-8",
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"name": "Fraunces",
					"slug": "fraunces",
					"fontFamily": "Fraunces, serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/fraunces/fraunces-v35-latin-700.woff2"
							],
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"fontFamily": "Fraunces"
						},
						{
							"src": [
								"file:./assets/fonts/fraunces/fraunces-v35-latin-700italic.woff2"
							],
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontDisplay": "swap",
							"fontFamily": "Fraunces"
						}
					]
				},
				{
					"name": "DM Sans",
					"slug": "dm-sans",
					"fontFamily": "\"DM Sans\", sans-serif",
					"fontFace": [
						{
							"src": [
								"file:./assets/fonts/dm-sans/dm-sans-v15-latin-regular.woff2"
							],
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"fontFamily": "\"DM Sans\""
						},
						{
							"src": [
								"file:./assets/fonts/dm-sans/dm-sans-v15-latin-italic.woff2"
							],
							"fontWeight": "400",
							"fontStyle": "italic",
							"fontDisplay": "swap",
							"fontFamily": "\"DM Sans\""
						},
						{
							"src": [
								"file:./assets/fonts/dm-sans/dm-sans-v15-latin-600.woff2"
							],
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"fontFamily": "\"DM Sans\""
						},
						{
							"src": [
								"file:./assets/fonts/dm-sans/dm-sans-v15-latin-600italic.woff2"
							],
							"fontWeight": "600",
							"fontStyle": "italic",
							"fontDisplay": "swap",
							"fontFamily": "\"DM Sans\""
						},
						{
							"src": [
								"file:./assets/fonts/dm-sans/dm-sans-v15-latin-700.woff2"
							],
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"fontFamily": "\"DM Sans\""
						},
						{
							"src": [
								"file:./assets/fonts/dm-sans/dm-sans-v15-latin-700italic.woff2"
							],
							"fontWeight": "700",
							"fontStyle": "italic",
							"fontDisplay": "swap",
							"fontFamily": "\"DM Sans\""
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.38rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.75rem"
					},
					"name": "Extra Large",
					"size": "1.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "2.6rem",
						"min": "1.4rem"
					},
					"name": "Extra Extra Large",
					"size": "2.6rem",
					"slug": "xx-large"
				}
			]
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var:preset|font-family|dm-sans",
			"fontSize": "var:preset|font-size|medium",
			"letterSpacing": "-0.01em",
			"lineHeight": "1.5"
		},
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontSize": "1rem"
				}
			},
			"core/post-title": {
				"typography": {
					"fontWeight": "700",
					"letterSpacing": "-0.96px"
				}
			},
			"core/pullquote": {
				"typography": {
					"fontSize": "var:preset|font-size|xx-large"
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var:preset|font-size|medium",
							"letterSpacing": "-0.14px"
						}
					}
				}
			},
			"core/quote": {
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var:preset|font-size|medium",
							"letterSpacing": "-0.14px"
						}
					}
				}
			},
			"core/query-title": {
				"typography": {
					"fontWeight": "900"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|dm-sans",
					"textTransform": "uppercase",
					"letterSpacing": "1.6px"
				}
			}
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var:preset|font-family|fraunces",
					"fontSize": "var:preset|font-size|small",
					"fontWeight": "700",
					"letterSpacing": "0.04em"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|fraunces",
					"fontWeight": "700",
					"lineHeight": "1.2"
				}
			}
		},
		"variations": {
			"post-terms-1": {
				"typography": {
					"fontSize": "var:preset|font-size|medium"
				}
			}
		}
	}
}
